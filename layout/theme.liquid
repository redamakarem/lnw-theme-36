        });
        $(window).scroll(function(){
          if(deviceWidth > 780){
            if($(window).scrollTop()>bannerHeight && $(window).scrollTop()<footerTop){
                scrollVal = $(window).scrollTop() - bannerHeight;
                if(scrollVal>(initialMarginTop * -1)){
